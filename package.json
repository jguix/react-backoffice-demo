    "eslint-plugin-react-refresh": "^0.4.6",
    "faker": "^6.6.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.2",
